@page
@model FirstRazerPagesUI.AdminLoginModel
@{
    ViewData["Title"] = "AdminLogin";
}

    <div class="container px-7">
        <form method="post">
            <div class="form-group plpr-form">
                <label for="exampleInputEmail1">Email address</label>
                <input type="email" asp-for="UserName" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" required>
                <small id="emailHelp" class="form-text text-muted">Access messages from contact page.</small>
            </div>
            <div class="form-group plpr-form">
                <label for="exampleInputPassword1">Password</label>
                <input type="password" asp-for="Password" class="form-control" id="exampleInputPassword1" required>
            </div>
            <button type="submit" class="btn btn-primary mlmr-form">Submit</button>
        </form>
    </div>
